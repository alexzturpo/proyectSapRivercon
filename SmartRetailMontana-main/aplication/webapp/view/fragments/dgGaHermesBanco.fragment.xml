<core:FragmentDefinition
xmlns="sap.m"
xmlns:core="sap.ui.core"
xmlns:mvc="sap.ui.core.mvc"   
xmlns:l="sap.ui.layout"
xmlns:tab="sap.ui.table"
xmlns:f="sap.ui.layout.form"
>
<Dialog contentWidth="40%" title="Generación de asientos POS a banco" showHeader="true" icon="sap-icon://sales-order" titleAlignment="Center">
    <f:Form editable="true" class="sapUiTinyMarginBottom" > 
        <f:layout> <f:ColumnLayout columnsM="2" columnsL="3" columnsXL="4" />  </f:layout>
        <f:formContainers>
            <f:FormContainer >  
                <f:formElements>
                    <f:FormElement label="Fecha valor">
                        <f:fields> <Input value=""/> </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="N° operación">
                        <f:fields> <Input value=""/> </f:fields>
                    </f:FormElement> 
                    <f:FormElement label="Importe">
                        <f:fields> <Input value=""/> </f:fields>
                    </f:FormElement> 
                </f:formElements>
            </f:FormContainer> 
        </f:formContainers>
    </f:Form> 
    <OverflowToolbar>
        <Button text="Aceptar" press="dgGaHermesBancoOk" type="Emphasized" />
        <Button text="Cancelar" press="dgCancelGaHermesBanco"/> 
    </OverflowToolbar>
</Dialog>
</core:FragmentDefinition>
