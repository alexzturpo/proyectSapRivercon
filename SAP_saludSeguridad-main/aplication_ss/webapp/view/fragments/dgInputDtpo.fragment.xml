<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
	<SelectDialog title="Departamento"
		items="{ path: 'myParam>/ZSYSO_DPTO'}"
		search="dgSearchDtpo"
		confirm="dgGetCloseDtpo"
		cancel="dgGetCloseDtpo">
		<StandardListItem
			icon="sap-icon://building"
			iconDensityAware="false"
			iconInset="false"
			title="{myParam>DESCRIP}"
			description="{myParam>DEPARTAM}" />
	</SelectDialog>
</core:FragmentDefinition> 