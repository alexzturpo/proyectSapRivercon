<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
	<SelectDialog title="Gerencias"
		items="{ path: 'myParam>/ZSYSO_GERENCIASet'}"
		search="dgSearchGerencia"
		confirm=".dgGetCloseGerencia"
		cancel=".dgGetCloseGerencia">
		<StandardListItem
			icon="sap-icon://building"
			iconDensityAware="false"
			iconInset="false"
			title="{myParam>STEXT}"
			description="{myParam>ORGEH}" />
	</SelectDialog>
</core:FragmentDefinition> 